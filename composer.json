{
    "name": "latrach/mcp-sinistre",
    "description": "MCP Server pour la gestion de dossiers sinistres basÃ© sur Symfony 7.3",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "symfony/console": "^7.3",
        "symfony/dotenv": "^7.3",
        "symfony/framework-bundle": "^7.3",
        "symfony/runtime": "^7.3",
        "symfony/yaml": "^7.3",
        "symfony/serializer": "^7.3",
        "symfony/validator": "^7.3",
        "symfony/filesystem": "^7.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "minimum-stability": "RC",
    "prefer-stable": true
}
